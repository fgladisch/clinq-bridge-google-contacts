{
	"name": "clinq-bridge-google",
	"version": "1.0.0",
	"private": true,
	"description": "CLINQ-Bridge for Google Contacts.",
	"main": "dist/index.js",
	"files": [
		"dist/**"
	],
	"repository": "git@github.com:sipgate/clinq-bridge-google.git",
	"author": "Felix Gladisch <felix@felixgladisch.de>",
	"license": "Apache-2.0",
	"scripts": {
		"postinstall": "tsc",
		"start": "node dist/index",
		"start:dev": "ts-node-dev src/index.ts",
		"build": "tsc",
		"gcp-build": "tsc && mv secrets.json dist",
		"lint": "tslint --project tsconfig.json",
		"secrets:encrypt": "gcloud kms encrypt --location global --keyring clinq-bridge-google --key prod --plaintext-file secrets.json --ciphertext-file secrets.json.encrypted",
		"secrets:decrypt": "gcloud kms decrypt --location global --keyring clinq-bridge-google --key prod --ciphertext-file secrets.json.encrypted --plaintext-file secrets.json"
	},
	"dependencies": {
		"@clinq/bridge": "^11.0.0",
		"@types/express": "^4.16.1",
		"@types/node": "^12.0.1",
		"googleapis": "^39.2.0",
		"typescript": "^3.4.5"
	},
	"devDependencies": {
		"ts-node-dev": "^1.0.0-pre.39",
		"tslint": "^5.16.0",
		"tslint-config-prettier": "^1.18.0"
	}
}
